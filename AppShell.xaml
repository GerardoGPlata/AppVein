<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AppVein.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AppVein"
    Title="Vein"
    BackgroundColor="{StaticResource VeinBackgroundColor}"
    ForegroundColor="{StaticResource VeinTextColor}"
    TitleColor="{StaticResource VeinTextColor}">

    <ShellContent
        Title="Inicio"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage"/>

</Shell>
